{
  "name": "amazon-html-to-json",
  "version": "1.0.0",
  "license": "MIT",
  "description": "parse( html from amazon detail ) => JSON",
  "keywords": [],
  "repository": {
    "type": "git",
    "url": "https://github.com/blairanderson/amazon-html-to-json.git"
  },
  "main": "index.js",
  "scripts": {
    "clean": "rimraf dist",
    "build": "babel src -d dist",
    "lint": "eslint src tests",
    "lint:fix": "npm run lint -- --fix",
    "test": "mocha tests --compilers js:babel-register",
    "test:dev": "npm run test -- --watch",
    "prepublish": "npm run clean & npm run build",
    "publish":
      "git add . && git commit -m 'publish' && git push origin master && echo https://github.com/blairanderson/amazon-html-to-json.git#\"$(git log --pretty=format:'%H' -n 1)\""
  },
  "devDependencies": {
    "babel-cli": "^6.7.5",
    "babel-core": "^6.7.6",
    "babel-eslint": "^6.0.2",
    "babel-plugin-transform-runtime": "^6.7.5",
    "babel-polyfill": "^6.7.4",
    "babel-preset-es2015": "^6.6.0",
    "chai": "^3.5.0",
    "eslint": "^2.7.0",
    "eslint-config-airbnb": "^7.0.0",
    "eslint-plugin-import": "^1.4.0",
    "mocha": "^2.4.5",
    "rimraf": "^2.5.2"
  },
  "dependencies": {
    "babel-runtime": "^6.6.1",
    "cheerio": "^1.0.0-rc.2",
    "deep-equal": "^1.0.1"
  }
}
